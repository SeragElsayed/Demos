

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FullCalenderDemo</title>

    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/site.css" />
        <link href="~/lib/font-awesome/css/all.min.css" rel="stylesheet" type="text/css" />*@
    <link href="~/AdminLTE/plugins/fontawesome-free/css/all.css" rel="stylesheet" />

    <link href="~/AdminLTE/dist/css/adminlte.css" rel="stylesheet" />


    @if (IsSectionDefined("AddToHead"))
    {
        @RenderSection("AddToHead", required: false)
    }
    <link rel="stylesheet" href="~/css/site.css" />

</head>
<body class="hold-transition sidebar-mini">
    <div class="wrapper">

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>



                <li class="nav-item d-none d-sm-inline-block">
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                </li>



            </ul>

            <!-- Right navbar links -->
            <partial name="_LoginPartial" />

        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a asp-area="" asp-controller="Home" asp-action="Index" class="brand-link">
                <img src="~/AdminLTE/dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">AdminLTE 3</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                @if (User.Identity.IsAuthenticated)
                {
                    <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                        @*<div class="image">
                            <img src="~/AdminLTE/dist/img/user8-128x128.jpg" class="img-circle elevation-2" alt="User Image">
                        </div>*@
                        <div class="info">
                            <a id="manage" class="nav-link text-light" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Hello @User.Identity.Name !</a>
                        </div>
                    </div>
                }



                <!-- Sidebar Menu -->




                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                        <li class="nav-item">
                            <a asp-area="" asp-controller="Event" asp-action="Index" class="nav-link">
                                <i class="nav-icon fas fa-calendar"></i>
                                <p>
                                    Calendar
                                </p>
                            </a>
                        </li>
                        @if (User.IsInRole("Admin") || User.IsInRole("SuperAdmin"))
                        {
                            <li class="nav-item menu-open">
                                <a href="#" class="nav-link active">
                                    <i class="nav-icon fas fa-cogs"></i>
                                    <p>
                                        Manage
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a asp-area="" asp-controller="Event" asp-action="GetAllEvents" class="nav-link ">
                                            <i class="far fa-calendar-check nav-icon"></i>
                                            <p>Events</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-area="" asp-controller="User" asp-action="Index" class="nav-link">
                                            <i class="far fa-user nav-icon"></i>
                                            <p>Users</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-area="" asp-controller="Customer" asp-action="Index" class="nav-link">
                                            <i class="nav-icon fas fa-address-book"></i>
                                            <p>
                                                Customers
                                            </p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-area="" asp-controller="Service" asp-action="Index" class="nav-link">
                                            <i class="nav-icon fas fa-concierge-bell"></i>
                                            <p>
                                                Services
                                            </p>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        }

                    </ul>
                </nav>
                <!-- /.sidebar-menu -->

            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">

            @RenderBody()

        </div>

    </div>


